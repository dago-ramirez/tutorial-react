    "autoprefixer": "^9.8.6",
    "postcss-cli": "^7.1.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "3.4.3",
    "tailwindcss": "^1.8.5"
  },
  "scripts": {
    "b:css": "postcss src/styles/tailwind.css -o src/index.css",
